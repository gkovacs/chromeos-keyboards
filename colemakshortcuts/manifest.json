
{
  "name": "Colemak with Shortcuts",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Colemak plus shortcuts mapped to disabled Alt key",
  "background":  {
    "scripts": ["main.js"]
  },
  "permissions": [
    "input"
  ],
  "input_components": [
    {
      "name": "Colemak with Shortcuts",
      "type": "ime",
      "id": "colemakshortcuts",
      "indicator": "C",
      "description": "Colemak with Shortcuts",
      "language": "en-US",
      "layouts": ["us::eng"]
    }
  ]
}